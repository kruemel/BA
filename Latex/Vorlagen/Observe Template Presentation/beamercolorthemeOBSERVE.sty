\mode<presentation>


\definecolor{beamer@obgrey}{rgb}{0.57,0.58,0.59}
\definecolor{beamer@obblue}{RGB}{0,0,170}

\definecolor{beamer@hawdarkblue}{RGB}{2,56,120}
\definecolor{beamer@hawlightblue}{RGB}{152,191,230}

\setbeamercolor{normal text}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=red}
\setbeamercolor{example text}{fg=green!50!black}

\setbeamercolor{structure}{fg=beamer@obblue,bg=beamer@obgrey!30}

\setbeamercolor{background canvas}{parent=normal text}
\setbeamercolor{background}{parent=background canvas}


\setbeamercolor{palette primary}{use=structure,fg=structure.fg, bg=structure.bg}
\setbeamercolor{palette secondary}{use=structure,fg=structure.fg!75!black, bg=structure.bg}
%\setbeamercolor{palette tertiary}{use=structure,fg=structure.fg!30!black, bg=structure.bg}
\setbeamercolor{palette tertiary}{use=structure,bg=structure.fg, fg=structure.bg}
\setbeamercolor{palette quaternary}{fg=black}


\setbeamercolor{palette sidebar primary}{use=normal text,fg=normal text.fg}
\setbeamercolor{palette sidebar secondary}{use=structure,fg=structure.fg}
\setbeamercolor{palette sidebar tertiary}{use=normal text,fg=normal text.fg}
\setbeamercolor{palette sidebar quaternary}{use=structure,fg=structure.fg}


\setbeamercolor{math text}{}
